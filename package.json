{
  "name": "start-repo",
  "private": true,
  "version": "1.0.0",
  "description": "Web application to easily start a new repository with a bunch of default issues.",
  "engines": {
    "node": ">=18.20.4",
    "npm": "please-use-yarn",
    "yarn": ">=1.22"
  },
  "scripts": {
    "prepare": "husky install && yarn prisma generate",
    "postinstall": "yarn theme:generate-typing",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pretty": "prettier -w .",
    "lint": "eslint ./src --fix && tsc --noEmit",
    "lint-staged": "eslint --cache --fix && tsc --noEmit",
    "storybook": "storybook dev --no-open -p 6006",
    "storybook:build": "storybook build && mv ./storybook-static ./public/storybook",
    "static:build": "yarn build && next export",
    "static:serve": "yarn static:build && npx servor out app.html 5000",
    "theme:generate-typing": "chakra-cli tokens ./src/theme/theme.ts",
    "dk:init": "docker compose up -d && sleep 10 && yarn db:push",
    "dk:start": "docker compose start",
    "dk:stop": "docker compose stop",
    "db:ui": "prisma studio",
    "db:push": "prisma db push"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "prettier --write",
    "*.{ts,tsx,js,jsx}": "yarn lint-staged"
  },
